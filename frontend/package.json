    "eslint-plugin-react": "7.33.2",
    "fast-glob": "3.3.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.1.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.31",
    "postcss-nesting": "12.0.1",
    "prettier": "3.1.0",
    "process": "^0.11.10",
    "start-server-and-test": "2.0.3",
    "typed-css-modules": "0.8.1",
    "typescript": "5.3.2"
  }

{
  "name": "console",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "build:css-types": "tcm --c -e -p  'src/**/*.css' .",
    "copy:css": "node ./scripts/copy-css.cjs",
    "dev": "node scripts/dev.cjs",
    "lint": "eslint console --ext .ts,.tsx,.js,.cjs",
    "lint:fix": "npm run lint -- --fix --quiet",
    "plop": "npx plop",
    "preview": "vite preview",
    "storybook": "storybook dev --no-open -p 6006",
    "test": "jest ./src",
    "test:coverage": "jest ./src --coverage",
    "test:playwright": "start-server-and-test 'yarn run storybook:ci' http://localhost:9000 'npx playwright test'",
    "test:playwright-update-snapshots": "start-server-and-test 'yarn run storybook:ci' http://localhost:9000 'npx playwright test --update-snapshots'",
    "test:watch": "jest ./src --watch --verbose",
    "vite": "vite"
  },
  "lint-staged": {
    "*.(js|cjs|tsx|ts)": [
      "npm run lint:fix"
    ],
    "package.json": [
      "format-package -w"
    ]
  },
  "browserslist": [
    "> 2%"
  ],
  "dependencies": {
    "@buf/bufbuild_eliza.bufbuild_connect-es": "^0.8.6-20221224180530-f3801d450ef9.1",
    "@bufbuild/connect": "^0.8.6",
    "@bufbuild/connect-web": "^0.8.6",
    "@bufbuild/protobuf": "^1.2.0",
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0",
    "react-syntax-highlighter": "^15.5.0",
    "reactflow": "^11.7.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "eslint": "8.46.0",
    "eslint-plugin-compat": "4.1.4",
    "eslint-plugin-react": "7.33.1",
    "eslint-plugin-storybook": "0.6.13",
    "format-package": "7.0.0",
    "@axe-core/playwright": "4.7.3",
    "@bufbuild/buf": "^1.15.0-1",
    "@bufbuild/protoc-gen-connect-es": "^0.8.6",
    "@bufbuild/protoc-gen-es": "^1.2.0",
    "@jest/globals": "29.5.0",
    "@playwright/test": "1.36.2",
    "@storybook/addon-essentials": "^7.2.1",
    "@storybook/addon-interactions": "^7.2.1",
    "@storybook/addon-links": "^7.2.1",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.2.1",
    "@storybook/jest": "0.1.0",
    "@storybook/react": "^7.2.1",
    "@storybook/react-vite": "^7.2.1",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/testing-react": "2.0.1",
    "@swc/core": "1.3.67",
    "@swc/jest": "0.2.26",
    "@testing-library/react": "14.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "chokidar": "3.5.3",
    "fast-glob": "3.3.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.6.2",
    "npm-run-all": "4.1.5",
    "playwright": "1.36.2",
    "plop": "3.1.2",
    "postcss": "^8.4.23",
    "start-server-and-test": "2.0.0",
    "storybook": "7.2.2",
    "tailwindcss": "^3.3.2",
    "typed-css-modules": "0.7.2",
    "typescript": "5.1.6",
    "vite": "4.4.9"
  }
}
{
  "name": "console",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "source": "index.html",
  "scripts": {
    "build": "tsc && parcel build",
    "build:css-types": "tcm --c -e -p  'src/**/*.css' .",
    "copy:css": "node ./scripts/copy-css.cjs",
    "dev": "node scripts/dev.cjs",
    "lint": "eslint . --ext .ts,.tsx,.js,.cjs",
    "lint:fix": "npm run lint -- --fix --quiet",
    "plop": "npx plop",
    "preview": "vite preview",
    "storybook": "storybook dev --no-open -p 6006",
    "test": "jest ./src",
    "test:coverage": "jest ./src --coverage",
    "test:playwright": "start-server-and-test 'yarn run storybook:ci' http://localhost:9000 'npx playwright test'",
    "test:playwright-update-snapshots": "start-server-and-test 'yarn run storybook:ci' http://localhost:9000 'npx playwright test --update-snapshots'",
    "test:watch": "jest ./src --watch --verbose",
    "parcel": "parcel --port 5173"
  },
  "lint-staged": {
    "*.(js|cjs|tsx|ts)": [
      "npm run lint:fix"
    ],
    "package.json": [
      "format-package -w"
    ]
  },
  "browserslist": [
    "> 2%"
  ],
  "dependencies": {
    "@bufbuild/connect": "0.12.0",
    "@bufbuild/connect-web": "0.12.0",
    "@bufbuild/protobuf": "1.3.0",
    "@headlessui/react": "1.7.16",
    "@heroicons/react": "2.0.18",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.15.0",
    "react-syntax-highlighter": "15.5.0",
    "reactflow": "11.8.2"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.7.3",
    "@bufbuild/buf": "1.26.1",
    "@bufbuild/protoc-gen-connect-es": "0.12.0",
    "@bufbuild/protoc-gen-es": "1.3.0",
    "@jest/globals": "29.6.2",
    "@playwright/test": "1.37.0",
    "@swc/core": "1.3.77",
    "@swc/jest": "0.2.29",
    "@testing-library/jest-dom": "6.0.0",
    "@testing-library/react": "14.0.0",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "autoprefixer": "10.4.15",
    "chokidar": "3.5.3",
    "eslint": "8.47.0",
    "eslint-plugin-compat": "4.1.4",
    "eslint-plugin-react": "7.33.2",
    "fast-glob": "3.3.1",
    "format-package": "7.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.6.2",
    "jest-environment-jsdom": "29.6.2",
    "lint-staged": "14.0.0",
    "npm-run-all": "4.1.5",
    "parcel": "2.9.3",
    "playwright": "1.37.0",
    "postcss": "8.4.28",
    "postcss-modules": "^4.3.1",
    "process": "^0.11.10",
    "start-server-and-test": "2.0.0",
    "tailwindcss": "3.3.3",
    "typed-css-modules": "0.7.2",
    "typescript": "5.1.6"
  }
}
